<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="PathToPhantom" value="path\to\phantomjs.exe"/>
    <add key="PhantomScript" value="path\to\phantomWS.js" />
    <add key="PhantomLogLocation" value="path\to\Components\" />
    <add key="PhantomPort" value="7090"/>
    <add key="LocalHost" value="http://localhost"/>
    <add key="CreateSnapshoots" value="1"/>
	<add key="SanpshootLocation" value="path\to\Sanpshoots\"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>